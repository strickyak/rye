func (inline) DFromint(i int) (D, D) {
	return D(1), D(i)
}

func (inline) DFromInt(i int64) (D, D) {
	return D(1), D(i)
}

func (inline) IntFromD(dx, dy D) int64 {
  _ = dx
	return int64(dy)
}

func (inline) AddD(ax D, ay D, bx D, by D) (D, D) {
	return macro.DFromInt(macro.IntFromD(ax, ay) + macro.IntFromD(bx, by))
}


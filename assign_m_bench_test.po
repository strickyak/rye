func add(a, b int) int {
	return a + b
}
func addptr(a, b uintptr) uintptr {
	return a + b
}

func AddIntPtrFast(a uintptr, b uintptr) uintptr {
	return addptr(a, b)
}

func AddIntPtrSlow(a uintptr, b uintptr) uintptr {
	a1 := a
	a2 := a1
	a3 := a2

	b1 := b
	b2 := b1
	b3 := b2

	z := addptr(a3, b3)

	z1 := z
	z2 := z1
	z3 := z2

	return z3
}

